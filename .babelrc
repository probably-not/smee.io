{
  "presets": [
    "@babel/env",
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/env",
          {
            "exclude": [
              "transform-classes"
            ]
          }
        ],
        "@babel/react"
      ]
    }
  }
}